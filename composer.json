{
    "name": "20tries/date_range",
    "description": "An package for handling date ranges.",
    "keywords": [
        "date", "date_range", "range", "timeperiod", "carbon"
    ],
    "homepage": "https://github.com/20tries/date_range",
    "license": "MIT",
    "require": {
        "php" : ">=7.2",
        "nesbot/carbon": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "illuminate/support": "5.2.*",
        "symfony/var-dumper": "3.0.*",
        "symfony/debug": "3.0.*"
    },
    "autoload": {
        "psr-4": {
            "_20TRIES\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "_20TRIES\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        }
    }
}
